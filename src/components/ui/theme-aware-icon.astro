---
import { cn } from "@/lib/utils";

interface Props {
  lightIcon: any;
  darkIcon: any;
  class?: string;
}

const { lightIcon: LightIcon, darkIcon: DarkIcon, class: className = "" } = Astro.props;
---

<LightIcon class={cn("dark:hidden", className)} />
<DarkIcon class={cn("hidden dark:block", className)} />
