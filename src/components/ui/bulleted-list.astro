---
import { cn } from "@/lib/utils";
import type { HTMLAttributes } from "astro/types";

interface Props extends HTMLAttributes<"ul"> {}

const { class: classOverrides, ...props } = Astro.props;
---

<ul
  class={cn("list-disc list-inside marker:text-primary", classOverrides)}
  {...props}
>
  <slot />
</ul>
